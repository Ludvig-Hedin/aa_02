# AA_02 Environment Configuration
# IMPORTANT: Rename this file to .env and fill in your actual keys
# DO NOT commit the .env file to version control

# ==============================
# Supabase Configuration
# ==============================
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# ==============================
# Vercel Configuration
# ==============================
VERCEL_PROJECT_ID=your_vercel_project_id
VERCEL_TEAM_ID=your_vercel_team_id
VERCEL_API_TOKEN=your_vercel_api_token

# ==============================
# AI Model API Keys
# ==============================

# OpenAI (ChatGPT)
OPENAI_API_KEY=your_openai_api_key
OPENAI_ORG_ID=your_openai_org_id

# Anthropic (Claude)
ANTHROPIC_API_KEY=your_anthropic_api_key

# Google (Gemini)
GOOGLE_AI_API_KEY=your_google_ai_api_key

# ==============================
# Local Model Configuration
# ==============================

# Deepseek Configuration
DEEPSEEK_MODEL_PATH=path_to_deepseek_model
DEEPSEEK_MAX_TOKENS=4096

# Qwen Configuration
QWEN_MODEL_PATH=path_to_qwen_model
QWEN_MAX_TOKENS=2048

# Llama Configuration
LLAMA_MODEL_PATH=path_to_llama_model
LLAMA_MAX_TOKENS=2048

# ==============================
# Integration Services
# ==============================

# Calendar Integration
GOOGLE_CALENDAR_CLIENT_ID=your_google_calendar_client_id
GOOGLE_CALENDAR_CLIENT_SECRET=your_google_calendar_client_secret

# WebRTC Configuration
TURN_SERVER_URL=your_turn_server_url
TURN_SERVER_USERNAME=your_turn_server_username
TURN_SERVER_CREDENTIAL=your_turn_server_credential

# ==============================
# Application Configuration
# ==============================

# Node Environment
NODE_ENV=development # Change to production for deployment

# API Rate Limiting
MAX_REQUESTS_PER_MINUTE=60

# Default Model Selection
DEFAULT_MODEL=claude # Options: claude, chatgpt, gemini, deepseek, qwen, llama

# Enable/Disable Features
ENABLE_BROWSER_INTEGRATION=true
ENABLE_RESEARCH_CAPABILITIES=true
ENABLE_SCREEN_SHARING=true
ENABLE_VOICE_CONTROL=true
ENABLE_VIDEO_GENERATION=true


# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# AI Model API Keys
NEXT_PUBLIC_ANTHROPIC_API_KEY=your_anthropic_api_key
NEXT_PUBLIC_OPENAI_API_KEY=your_openai_api_key
NEXT_PUBLIC_GOOGLE_API_KEY=your_google_api_key

# Optional: Local Model Configuration
# LOCAL_MODEL_PATH=/path/to/local/models
# LOCAL_MODEL_PORT=8080 